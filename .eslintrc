{
  "extends": [
    "eslint-config-airbnb",
    "eslint-config-airbnb/hooks"
  ],
  "env": {
    "browser": true,
    "es2020": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "settings": {
    "react": {
        "version": "999.999.999"
    }
  },
  "rules": {
    "max-len": ["error", { "code": 120 }],
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "no-param-reassign": 0,
    "func-names": 0,
    "no-plusplus": 0,
    "arrow-parens": 0,
    "consistent-return": 0,
    "import/no-extraneous-dependencies": 0,
    "react/react-in-jsx-scope": 0,
    "react/button-has-type": 0,
    "no-array-index-key": 0,
    "import/no-unresolved": ["error", { "ignore": ["\\.(css|less|scss|sass)$"] }],
    "no-nested-ternary": 0
  }
}
